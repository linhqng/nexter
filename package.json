{
  "name": "Nexter",
  "version": "1.0.0",
  "description": "Nexter webpage",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "watch:pug": "pug -w ./pug -o . -P",
    "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:pug watch:sass"
  },
  "author": "Linh Nguyen",
  "license": "ISC",
  "dependencies": {
    "pug": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  }
}
